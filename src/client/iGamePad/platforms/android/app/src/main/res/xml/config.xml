<?xml version='1.0' encoding='utf-8'?>
<widget id="sph.el.games.iGamePad" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>iGamePad</name>
    <description>Gamepad to Rock Paper Scissors game</description>
    <author email="emoliveira@jenetwork.com.br" href="http://sphelgames.jenetwork.com.br/">
        Sph El Games
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <preference name="loglevel" value="DEBUG" />
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
</widget>
